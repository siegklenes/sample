{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "commonjs",
    "target": "es5",
    "types": [
      "jasmine",
      "jasminewd2",
      "node",
      "@wdio/jasmine-framework",
      "@wdio/sync"
    ],
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
